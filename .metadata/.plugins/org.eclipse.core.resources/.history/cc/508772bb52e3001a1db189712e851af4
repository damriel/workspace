#include <stdio.h>
#include <stdlib.h>
#include "utn.h"

int main(void) {
	setbuf(stdout,NULL); //ESTO VA SIEMPRE EN WINDOWS

	int edad;
	int respuesta;
	respuesta = int utn_getIntLimited(&edad, "Edad? \n", "Error, la edad debe ser desde 0 a 199\n", 0, 199, 2);
	if(respuesta == 0)
	{
		printf("La edad es: %d",edad);
	}
	else
	{
		printf("ERROR.");
	}


	return EXIT_SUCCESS;
}
